<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zanakay">
    
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/apphianaro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Zanakay"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config">
        <uses-library android:name="org.apache.http.legacy" android:required="false"/>
        <activity
            android:name=".vue.VideoView"
            android:exported="true" />
        <activity
            android:name=".ListeVideo"
            android:exported="true" />
        <activity
            android:name=".vue.FragmentActivity"
            android:exported="true" />
        <activity
            android:name=".Settings"
            android:exported="true" />
        <activity
            android:name=".vue.Recherche"
            android:exported="true" />
        <activity
            android:name=".vue.CategorieSante"
            android:exported="true" />
        <activity
            android:name=".vue.ArticleFiche"
            android:exported="true"
            android:label="@string/title_activity_article_fiche"
            android:theme="@style/Theme.Zanakay.NoActionBar" />
        <activity
            android:name=".vue.ListeArticleApprentissage"
            android:exported="true" />
        <activity
            android:name=".vue.CategorieApprentissage"
            android:exported="true" />
        <activity
            android:name=".vue.Menu"
            android:exported="true" />
        <activity
            android:name=".vue.SignUp"
            android:exported="true" />
        <activity
            android:name=".vue.Login"
            android:exported="true" />
        <activity
            android:name=".vue.Dashboard"
            android:exported="true" />
        <activity
            android:name=".vue.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>